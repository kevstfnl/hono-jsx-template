{
	"name": "@kevstfnl/hono-mono-template",
	"author": "Kevin Stefanelli",
	"description": "A template project for hono+jsx+bun",
	"private": true,
	"module": "src/index.ts",
	"type": "module",
	"packageManager": "^bun@1.2.20",
	"scripts": {
		"dev": "bun --watch ./src",
		"check": "bunx biome check --write",
		"build": "bun build ./src --outdir ./build",
		"build:minify": "bun run build --minify",
		"preview": "bun ./build"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/bun": "latest",
		"lightningcss": "^1.30.1",
		"lightningcss-cli": "^1.30.1"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"hono": "^4.9.7"
	}
}
